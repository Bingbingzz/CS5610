<%- include("partials/header"); -%>
  <h1>Home</h1>
  <p><%= startingContent %></p>


  <form class="" action="/compose">
    <button name="add">Add Blog</button>
  </form>

  <% posts.forEach(function(post){ %>
    <h1><%=post.title%></h1>
    <p><%=post.content.substring(0, 100) + " ..."%>
      <a href="/posts/<%= post._id %>">Read more</a>
    </p>
  <% }); %>

<%- include("partials/footer"); -%>
